%h1 Customers 
%table
  -@customers.each do |customer|
    %tr
      %td
        =link_to customer.full_name, customer
      %td
        =customer.address
      %td
        =customer.city
      %td
        =customer.state
      %td
        =customer.zip
      %td
        =customer.contact
%br
=link_to "Add customer", new_customer_path
